<% include partials/header %>

	<h1>Campgrounds Page</h1>

	<div class="campground_feed">
		<% campgrounds.forEach((e)=>{ %>
			<div class="campground">
				<a href="/campgrounds/<%= e._id %>">
					<img src="<%= e.image %>" alt="campground image" class='camp_img'>
					<p class="camp_title"><%= e.name %></p>
				</a>	
			</div>
		<% }); %>
	</div>

	<div>
		<a href="/campgrounds/new">Add a new campground!</a>
	</div>

<% include partials/footer %>