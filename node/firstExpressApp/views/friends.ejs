<% include partials/header %>

<h1>The Friends Page!</h1>

<p>Below are all my friends:</p>
<ul>
	<% for(var i = 0; i < friends.length; i++){ %>
		<li><%= friends[i] %></li>
	<% } %>
</ul>

<p>Add a new friend!</p>

<form action="/addFriend" method="POST">
	<input type="text" name="newFriend" id="name" placeholder="Name">
	<input type="submit">
</form>

<% include partials/footer %>